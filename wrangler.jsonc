{
  // ğŸŒ´ CharlestonHacks Events Proxy Worker
  // --------------------------------------
  // Aggregates Charleston tech + entrepreneurial events from multiple sources.
  // Auto-refreshes every 3 hours and caches results in KV for fast access.

  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "charlestonhacks-events-proxy",
  "main": "src/index.js",
  "compatibility_date": "2024-11-13",

  // âœ… KV namespace for caching event data
  "kv_namespaces": [
    {
      "binding": "CACHE",
      "id": "5e7c639f4a6a42339172bf891a14d257"  // â† use your real KV namespace ID
    }
  ],

  // ğŸ•’ Cron trigger: run every 3 hours (auto refresh)
  "triggers": {
    "crons": ["0 */3 * * *"]
  },

  // âš™ï¸ Environment variables
  "vars": {
    "TZ": "America/New_York"
  },

  // ğŸŒ Enable NodeJS compatibility for HTML parsing
  "compatibility_flags": ["nodejs_compat"],

  // ğŸš€ Deployment model
  "usage_model": "bundled",

  // ğŸ“Š Enable Cloudflare dashboard observability (optional)
  "observability": {
    "enabled": true
  }
}
